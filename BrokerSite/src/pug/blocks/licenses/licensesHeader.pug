header.page-header.licenses-header
	.page-header__box_1
		img.page-header__img(src=baseDir + ["assets/images/licenses-header.png"] alt="licenses-header" title="licenses-header")
	.page-header__box_2
		.wrapper
			.page-header__item
				.page-header__wrapper
					h1.page-header__title.h1-title !{licensesHeaderText.title[lang]}
					.page-header__description
						.page-header__rect
						p.page-header__text.description !{licensesHeaderText.text[lang]}
					a(href=linkSignup).btn.btn-main.btn-big !{buttonText.btnStartTrade[lang]}
